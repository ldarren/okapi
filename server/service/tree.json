{
	"spec": {
		"tree": {
			"type": "array",
			"specs": [
				{"type": "string", "required": 1, "force": 1},
				"object",
				{"type": "array", "spec": ["ref", "_"]}
			]
		}
	},
	"rsc": {
		"tree": {
			"db": "db",
			"schema": {
				"type": "array",
				"specs": ["string", "object", {"type": "array", "spec": ["ref", "_"]}]
			}
		}
	},
	"routes":{
		"NEW/tree": [
			[["web.output"], "_.res", "_.output"],
			["web.bodyParser", "_.req", "_.:body"],
			[["util.input", "@.spec.tree"], "_.:body", "_.:input", "@.spec.tree"],
			["db.set", "db", "tree", null, "_.:input", "_.output"]
		],
		"GET/tree/:i": [
			[["web.output"], "_.res", "_.:output"],
			[["util.networkInterface", null, {"family":"IPv4", "internal":false}], "address", "_.:output"]
		],
		"/1.0/tree": [
			["@.tree.router",  "_.req.method"]
		],
		"/1.0/tree/:i": [
			["@.tree.router",  "_.req.method", "$.params.i"]
		]
	}
}
