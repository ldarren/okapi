{
	"spec": {
		"tree_node": {
			"name": {
				"type": "string",
				"gt": 0,
				"lt": 64
			},
			"env": "object"
		},
		"tree_leaf": {
			"name": {
				"type": "string",
				"gt": 0,
				"lt": 64
			},
			"method": "object",
			"url": "string",
			"headers": {
				"type": "array",
				"spec": "string"
			},
			"body": "string",
			"res": "string"
		}
	},
	"rsc": {
		"tree": {
			"meta": {
			},
			"schema": {
				"type": "object",
				"spec": {
					"id": {
						"type": "string",
						"gt": 0,
						"lt": 64
					},
					"tree_id": {
						"type": "string",
						"gt": 5,
						"lt": 64
					},
					"child": {
						"type": "array",
						"spec": {
							"type": "string",
							"gt": 5,
							"lt": 64
						}
					},
					"body": {
						"type": "object"
					},
					"cred": "object"
				}
			}
		}
	},
	"routes":{
		"LIST/tree": [
			[["web.output"], "_.res", "hello", {"page": 1}]
		],
		"GET/tree/:id": [
			[["util.input", "@.spec.params"], "$.params", "_.input"],
			["db.get", "tree", "_.input.id", "_.output"],
			[["web.output"], "_.res", "_.output", {"page": 1}]
		],
		"NEW/tree": [
			["web.bodyParser", "_.req", "_.body"],
			[["util.input", "@.rsc.tree.schema"], "_.body", "_.input"],
			["db.set", "tree", null, "_.input", "_.output"],
			["util.log", "_.output"],
			[["web.output"], "_.res", "_.output", {"page": 1}]
		],
		"/1.0/tree": [
			["@.tree.router",  "_.req.method", "$.params.id"]
		],
		"/1.0/tree/:id": [
			["@.tree.router",  "_.req.method", "$.params.id"]
		]
	}
} 
