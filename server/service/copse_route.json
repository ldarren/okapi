{
	"routes":{
		"GET/copse/id": [
            ["db.get", "$.db.user", "_.user.i", "_.user"],
			["@.copse.fRecord2CRDT", "_.record", "_.data"],
			["crdt.join", "$.crdt", "_.copse", "_.user", "_.data", "_.output"],
			["crdt.payload", "_.copse.id", "_.base64"],
			["crdt.send", "$.db.queue", "_.copse.id", "all", "_.base64.payload", "_.user", "_.user", "_.output"]
		],
		"PUT/copse/id": [
            ["crdt.verify", "_.params.key", "_.user.i"],
			["crdt.get", "_.params.key", "_.status"]
		],
		"DELETE/copse/id": [
            ["crdt.verify", "_.params.key", "_.user.i"],
			["crdt.remove", "_.params.key", "_.user.i", "_.status"]
		]
	}
}
